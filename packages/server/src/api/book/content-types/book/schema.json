{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "singularName": "book",
    "pluralName": "books",
    "displayName": "Book"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "cover": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "description": {
      "type": "text"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "layout": {
      "type": "enumeration",
      "enum": [
        "Book"
      ],
      "required": true
    },
    "stack_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::book-stack.book-stack"
    }
  }
}
